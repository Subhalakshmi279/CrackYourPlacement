Insert search position

class Solution {
    public int searchInsert(int[] nums, int target) {

        int n = nums.length;

        int ind = 0;

        for(int i=0;i<n;i++)
        {
                if(nums[i] == target)
                {
                    return i;
                }
                if(nums[i] > target)
                {
                    return i;
                }
        }

        return n;
        
    }
}
